    
    <div id="temp-spacer"></div>
    
    <div id="dash-container" data-user='{{userId}}' class="container">
        <h2>Welcome <span id="uName-span">{{user.username}}</span>!</h2>


        <div id="mood-row" class="row">
            <div>
                <h3>How are you feeling?</h3>
                <div id="mood-partial">
                    {{> mood moods=moods}}
                </div>
            </div>
            <div>
                <h3 id='hiddenPlaceH'><i class="material-icons left">directions_bike</i>Activity</h3>
                {{!-- <h3 id='hiddenPlaceH'><i class="material-icons left">directions_bike</i>Activity</h3> --}}
            <div id="suggested-activity-div" >
                
                
               
            </div>




            </div>
        </div>
        <div class="row">
            {{!-- column for chat and community board --}}
            <div class="col s6">
                <div class="row">
                    <div class="placeholder">
                        <h4>Our Community:</h4>                 
                        {{#each posts as |post|}}
                        {{> community}}
                        {{/each}}
                    </div>
                </div>
            </div>

            {{!-- column for mood and activity stuff --}}
            <div id="chat-col" class="col s6">
                {{> chat username=user.username}}
                <button id="join-chat-btn" class="waves-effect waves-light btn">Join Chat</button>
            </div>
        </div>
    </div>

    <script src="/js/getActivity.js"></script>
    {{!-- <script src="/js/activityResult.js"></script> --}}
    <script src="/js/forChat.js"></script>
    


